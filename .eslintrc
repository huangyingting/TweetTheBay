{
  env: {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "jest": true,
  },
  "plugins": [
    "jsx-a11y",
    "flowtype",
    "import",
  ],
  "parser": "babel-eslint",
  "extends": [
    "plugin:jsx-a11y/recommended",
    "airbnb",
  ],
  "rules": {
    "import/no-unresolved": [2, { caseSensitive: false }]
  },
}
